	}
	#portal-column-one {	width:  &dtml-SubSkinsWidth2;;
	padding: 0 20px 0 20px;	color: #ccc;	background: &dtml-SubSkinsColor6;;}#portal-column-one * {	margin: 0;}#portal-column-two {	width:  &dtml-SubSkinsWidth3;;	padding: 0 20px 0 20px;	color: #ccc;	background: white;}#portal-column-two * {	margin: 0;}#portal-column-content {	padding: 25px;}/* @end *//* </dtml-with> */