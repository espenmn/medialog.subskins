    directory="medialog.subskins:skins/medialog_subskins_styles"/>

 <object name="subskins_base"
    meta_type="Filesystem Directory View"
    directory="medialog.subskins:skins/subskins_base"/>
 <object name="subskins_top"
    meta_type="Filesystem Directory View"
