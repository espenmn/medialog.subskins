/* @override http://kjeldstad.medialog.no/portal_css/subskins/SubSkins_text_corporate.css */

/* <dtml-with get_base_properties mapping> (do not remove this :) */
/* <dtml-call "REQUEST.set('portal_url', portal_url())"> (not this either :) */

body {
	font-family: "Trebuchet MS", "Lucida Grande", Verdana, Lucida, Tahoma, Helvetica, Arial, sans-serif;
	font-size: 0.75em;
}

h1 {
	color: black;
	font-size: 180%;
	font-weight: normal;
	border-left: 18px double &dtml-SubSkinsColor1;;
	padding-left: 8px;
	margin-top: 13px;
	margin-bottom: 5px;
}

h2 {
	padding-top: 13px;
	font-size: 135%;
	font-weight: normal;
}

h3, h4, h5 {
	padding-top: 10px;
	font-size: 120%;
	font-weight: normal;
}

.documentDescription {
	padding: 4px 0px 0px 0px;
	color: black;
	font-weight: bold;
}

a {
	color: &dtml-SubSkinsColor2;;
}

p a:link {
	color: #233d5a;
	text-decoration: none;
	border-bottom: 1px solid &dtml-SubSkinsColor2;;
}

#content a:hover {
	color: &dtml-SubSkinsColor3;;
	}

img {
	border: none;
}


img.tileImage {
	float: left;
	margin: 8px 20px 10px 0px;
}

img.image-right {
	float: right;
	margin: 8px 0 10px 20px;
}

img.image-left {
	float: left;
	margin: 8px 20px 10px 0;
}


span#parent-fieldname-imageCaption {
	padding-right: 20px;
}


input {
	border: 1px solid #2f4355;
}

#content ul  {
	margin: 0.5em 0 0.75em 2em;
	list-style-type: disc;
}

ol {
	margin: 0.5em 0 0.75em 2em;
}

#content li {
	margin: 0.5em 0;
}

dl {
	margin: 0.5em 1em 0.75em;
}

dt {
	margin: 0.75em 0;
	font-weight: bold;
}

dd {
	margin: 0.5em 0;
}

a.tile {
	color: black;
}

fieldset {
	padding: 1em;
	border: 1px solid #ccc;
}

legend {
	font-weight: bold;
}

form {
	border: 0;
}

label {
	font-weight: bold;
}

textarea {
	border: 1px solid #ccc;
}

input {
	padding: 2px;
	border: 1px solid #ccc;
	background: #fff;
}

select {
	border: 1px solid #ccc;
}

.field {
	margin-top: 0.75em;
}

.formControls input, .searchButton {
	padding: 2px;
	padding-left: 20px;
}

table {
	border-collapse: collapse;
	font-size: 1em;
}

tbody {
	
}

tr {
	
}

th {
	
}

td {
	
}

em {
	font-style: normal;
	font-weight: bold;
}

strong {
	font-weight: bold;
}

cite, q {
	
}

blockquote {
	margin: 0.5em 2em 0.75em 2em;
	padding: 0.5em 2em;
	border: 1px solid #ccc;
	background: #fff url("quotation.gif") top left no-repeat;
}

code, kbd {
	background: #333;
	color: #ccc;
	font-family: "Lucida console", Lucida, Monaco, "Courier New", Courier, monospace;
}

pre {
	margin: 0.5em 2em 0.75em 2em;
	padding: 0.5em;
	background: #333;
	color: #ccc;
	font-family: "Lucida console", Lucida, Monaco, "Courier New", Courier, monospace;
}



del {
	color: #999;
	text-decoration: overline;
}

.portalMessage {
	padding: 0.375em 1em;
	margin-left: 0;
	margin-right:  0;
	background: &dtml-SubSkinsColor5;;
	border: 1px solid #eb0000;
}

.documentActions ul {
	margin: 0;
	text-align: right;
}

.documentActions ul li {
	display: inline;
	padding-right: 0.5em;
}

.documentActions ul li a {
	border: 0;
}

.documentFirstHeading {
	color: &dtml-SubSkinsColor1;;
}

.documentDescription {
	padding: 0.5em;
	background: &dtml-SubSkinsColor5;;
	margin-bottom: 1.5em;
}

.comment {
	border: 1px solid #ccc;
	background: #f3f3f3;
}

.photoAlbumEntryWrapper {
	float: left;
}

.photoAlbumEntry {
float: left;
height: 185px;
width: 143px;
margin: 0em;
padding: 0px 6px 0px 9px;
text-align: center;
background-image: url(polaroid-single.png);
background-repeat: no-repeat;
}
.photoAlbumEntry img {
border: 1px solid #ccc;
display: block;
margin: 0 auto;
}
.photoAlbumEntryWrapper {
height: 130px;
width: 128px;
margin-bottom: 7px;
}
.photoAlbumEntry a {
display: block;
text-decoration: none;
font-size: 85%;
height: 169px;
width: 130px;
margin: 16px auto 0px;
}
.photoAlbumFolder {
background-image: url(polaroid-multi.png);
background-repeat: no-repeat;
}
.photoAlbumEntryTitle {
color: #76797c;
display: block;
overflow: hidden;
width: 128px;
height: 3.6em;
}


/* </dtml-with> */