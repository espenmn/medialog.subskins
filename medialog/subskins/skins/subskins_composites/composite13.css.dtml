/* - SubSkins_calendar_small145.css - */
@media screen {
/* portal_css/SubSkins_calendar_small145.css?original=1 */



.dayPopup {
background-color: &dtml-SubSkinsColor5;;
padding: 0.2em;
position: absolute;
visibility: hidden;
width: 12em;
z-index: 2;
border: 1px solid &dtml-SubSkinsColor4;;
}
.date {
font-weight: bold;
}
.ploneCalendar {
width: 100%;
border-collapse: separate;
border-spacing: 1px;
border: 1px solid &dtml-SubSkinsColor1; ;
margin: 0;
padding: 0;
background: #f2f2f2;
}
.portletCalendar dt.portletHeader {
text-align: center;
font-size: 11px;
padding: 0;
}
.portletCalendar .portletItem {
border-style: none !important;
border-width: 0;
padding:  0;
}
.ploneCalendar a:hover {
text-decoration: none;
}
.ploneCalendar th {
padding: 0.3em;
background: &dtml-SubSkinsColor1; url(subskins_getimage/%26dtml-SubSkinsColor1%3B/menu_item.gif) repeat-x left;
color: whitesmoke;
text-align: center;
font-weight: normal;
border-bottom: 1px solid  &dtml-SubSkinsColor1;;
}
.ploneCalendar td {
text-align: center;
color: #454545;
line-height: 1.3em;
}
.ploneCalendar .weekdays td {
text-align: center;
}
.ploneCalendar .event {
font-weight: bold;
background-color: &dtml-SubSkinsColor6;;
}
.ploneCalendar .todayevent {
background-color: &dtml-SubSkinsColor1;;
font-weight: bold;
color: whitesmoke;
}
.ploneCalendar .todaynoevent {
border-collapse: collapse;
border: 2px solid &dtml-SubSkinsColor1;;
}



}

/* - SubSkins_globalnav_classy_34.css - */
@media screen {
/* portal_css/SubSkins_globalnav_classy_34.css?original=1 */
#portal-globalnav {
clear: both;
display: block;
margin: 0px;
padding: 0;
height: 38px;
text-transform: uppercase;
font: normal 11px Verdana, Helvetica, Arial, "Helvetica Neue", Geneva, sans-serif;
border: 4px solid white;
background:  &dtml-SubSkinsColor3;;
border-left-style: none;
}
#portal-globalnav li {
display: block;
margin: 0;
float: left;
border-left: 3px solid white;
width: 100px;
text-align: center;
}
#portal-globalnav li a {
display: block;
line-height: 38px;
color: white;
}
#portal-globalnav li:hover,
#portal-globalnav li.selected  {
background:  &dtml-SubSkinsColor4;;
}

}


