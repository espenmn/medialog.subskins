margin-bottom: 2px;
margin-left: 10px;
margin-top: 0;
padding-top: 10px;
}
.portletLogin .portletItem a.tile {
padding: 0px;
}
.portletLogin  input.context{
padding: 5px 20px;
margin: 6px 0 0 10px;
}


}


